{
  "root": "./docs",
  "gitbook": "3.2.3",
  "title": "Electrode Native Documentation",
  "description": "Electrode Native Documentation",
  "plugins": [
    "theme-api",
    "prism",
    "github",
    "-highlight",
    "copy-code-button",
    "versions"
  ],
  "pluginsConfig": {
    "prism": {
      "css": ["prismjs/themes/prism-tomorrow.css"]
    },
    "versions": {
      "gitbookConfigURL":
        "https://raw.githubusercontent.com/electrode-io/electrode-native/master/book.json",
      "options": [
        {
          "value":
            "https://native.electrode.io/v/v0.20/",
          "text": "Electrode Native [v0.20]"
        },
        {
          "value":
            "https://native.electrode.io/v/v0.21/",
          "text": "Electrode Native [v0.21]"
        },
        {
          "value":
            "https://native.electrode.io/v/master/",
          "text": "Electrode Native [master]",
          "selected": true
        }
      ]
    },
    "theme-api": {
      "languages": [
        {
          "lang": "ios",
          "name": "iOS",
          "default": true
        },
        {
          "lang": "android",
          "name": "Android"
        }
      ]
    },
    "github": {
      "url": "https://github.com/electrode-io/electrode-native"
    }
  }
}
